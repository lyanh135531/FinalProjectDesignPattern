package finalproject.tdtu;

public class MarketingStaff extends Employee{
	private double sales;
	private double commission;
	public MarketingStaff() {
	}
	
	public MarketingStaff(String id, String name, int phone, String position, String task, double salary, double sales,
			double commission) {
		super(id, name, phone, position, task, salary);
		this.sales = sales;
		this.commission = commission;
	}

	public double getSales() {
		return sales;
	}

	public void setSales(double sales) {
		this.sales = sales;
	}

	public double getCommission() {
		return commission;
	}

	public void setCommission(double commission) {
		this.commission = commission;
	}

	@Override
	public String toString() {
		return "MarketingStaff [getId()=" + getId() + ", getName()=" + getName() + ", getPhone()=" + getPhone()
				+ ", getPosition()=" + getPosition() + ", getTask()=" + getTask() + ", getSalary()=" + getSalary()
				+ ", toString()=" + super.toString() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", sales=" + sales + ", commission=" + commission + "]";
	}
	
	
	
	
	
}
