package finalproject.tdtu;

public class Leader extends Employee{
	private double responsibility;

	public Leader() {
	}

	public Leader(String id, String name, int phone, String position, String task, double salary,
			double responsibility) {
		super(id, name, phone, position, task, salary);
		this.responsibility = responsibility;
	}

	public double getResponsibility() {
		return responsibility;
	}

	public void setResponsibility(double responsibility) {
		this.responsibility = responsibility;
	}

	@Override
	public String toString() {
		return "Leader [getId()=" + getId() + ", getName()=" + getName() + ", getPhone()=" + getPhone()
				+ ", getPosition()=" + getPosition() + ", getTask()=" + getTask() + ", getSalary()=" + getSalary()
				+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + ", responsibility=" + responsibility
				+ "]";
	}

	
	
	
}
